<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="taiji.org.donkeymgr.EditSecondPageActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="繁育情况"
        android:id="@+id/textView14"
        android:layout_gravity="right"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="屠宰单位"
        android:id="@+id/textView29"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView14"
        android:layout_alignRight="@+id/textView14"
        android:layout_alignEnd="@+id/textView14" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="屠宰地点"
        android:id="@+id/textView13"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView29"
        android:layout_alignRight="@+id/textView29"
        android:layout_alignEnd="@+id/textView29" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="屠宰时间"
        android:id="@+id/textView15"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView13"
        android:layout_alignRight="@+id/textView13"
        android:layout_alignEnd="@+id/textView13" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="分割情况"
        android:id="@+id/textView16"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView15"
        android:layout_alignRight="@+id/textView15"
        android:layout_alignEnd="@+id/textView15" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="加工情况"
        android:id="@+id/textView17"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView16"
        android:layout_alignRight="@+id/textView16"
        android:layout_alignEnd="@+id/textView16" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="质检情况"
        android:id="@+id/textView18"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView17"
        android:layout_alignRight="@+id/textView17"
        android:layout_alignEnd="@+id/textView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="出厂时间"
        android:id="@+id/textView19"
        android:layout_gravity="right"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView18"
        android:layout_alignRight="@+id/textView18"
        android:layout_alignEnd="@+id/textView18" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上一页"
        android:id="@+id/previousPageButton"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/saveButton"
        android:layout_toStartOf="@+id/saveButton" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/breedStatusEditText"
        android:layout_alignBaseline="@+id/textView14"
        android:layout_toRightOf="@+id/textView14"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/killDepartmentEditText"
        android:layout_alignBaseline="@+id/textView29"
        android:layout_alignLeft="@+id/breedStatusEditText"
        android:layout_alignStart="@+id/breedStatusEditText"
        android:layout_alignRight="@+id/breedStatusEditText"
        android:layout_alignEnd="@+id/breedStatusEditText" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/killPlaceEditText"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignLeft="@+id/killDepartmentEditText"
        android:layout_alignStart="@+id/killDepartmentEditText"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/splitEditText"
        android:layout_alignBaseline="@+id/textView16"
        android:layout_alignLeft="@+id/killPlaceEditText"
        android:layout_alignStart="@+id/killPlaceEditText"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/processEditText"
        android:layout_alignBaseline="@+id/textView17"
        android:layout_alignLeft="@+id/splitEditText"
        android:layout_alignStart="@+id/splitEditText"
        android:layout_alignRight="@+id/splitEditText"
        android:layout_alignEnd="@+id/splitEditText" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/qualityEditText"
        android:layout_alignBaseline="@+id/textView18"
        android:layout_alignLeft="@+id/processEditText"
        android:layout_alignStart="@+id/processEditText"
        android:layout_alignRight="@+id/processEditText"
        android:layout_alignEnd="@+id/processEditText" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/killTimeEditText"
        android:layout_alignBaseline="@+id/textView15"
        android:layout_alignLeft="@+id/splitEditText"
        android:layout_alignStart="@+id/splitEditText"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:editable="false"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:ems="10"
        android:id="@+id/factoryTimeEditText"
        android:layout_alignBaseline="@+id/textView19"
        android:layout_alignLeft="@+id/qualityEditText"
        android:layout_alignStart="@+id/qualityEditText"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:editable="false"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="确定"
        android:id="@+id/saveButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
